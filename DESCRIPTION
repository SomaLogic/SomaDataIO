Type: Package
Package: SomaDataIO
Title: Input/Output 'SomaScan' Data
Version: 6.0.0.9000
Authors@R: c(
    person(given = "Stu",
           family = "Field",
           role = c("aut", "cre"),
           email = "stu.g.field@gmail.com",
           comment = c(ORCID = "0000-0002-1024-5859")),
    person(given = "SomaLogic Operating Co., Inc.", role = c("cph", "fnd"))
